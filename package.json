{
  "author": "Buidler Labs Developers <devs@buidlerlabs.com>",
  "description": "Documentation accompanying the Strato Js library",
  "name": "@buidlerlabs/hedera-strato-docs",
  "scripts": {
    "build": "run-s build:bundle-docs-app build:docs-docusaurus",
    "build:bundle-docs-app": "rollup -c ./rollup.config.js",
    "build:docs-docusaurus": "docusaurus build --out-dir=./built",
    "clean": "docusaurus clear",
    "deploy:docs": "docusaurus deploy",
    "start:docs": "docusaurus start"
  },
  "keywords": [
    "StratoJs",
    "Hedera",
    "Hashgraph",
    "Smart Contracts"
  ],
  "license": "MIT",
